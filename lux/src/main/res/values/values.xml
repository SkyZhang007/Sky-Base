    <declare-styleable name="LuxBanner">
        <attr format="enum" name="lux_indicatorType">
            <enum name="white" value="0"/>
            <enum name="gray" value="1"/>
        </attr>
        <attr name="lux_indicatorShape"/>
        <attr name="lux_selectedSize"/>
        <attr name="lux_normalSize"/>
        <attr name="lux_indicatorSpace"/>
        <attr format="enum" name="lux_indicatorGravity">//indicator位置
            <enum name="center" value="0"/>
            <enum name="start" value="1"/>
            <enum name="end" value="2"/>
        </attr>
        <attr format="dimension" name="lux_indicatorBottomMargin"/>//indicator距离底部间距
        <attr format="dimension" name="lux_indicatorStartMargin"/>//indicator距离左边间距
        <attr format="dimension" name="lux_indicatorEndMargin"/>//indicator距离右边间距

        <attr format="integer" name="lux_autoScrollDuration"/>//轮播时间
        <attr format="boolean" name="lux_isAutoPlay"/>//是否自动播放
        <attr format="boolean" name="lux_isCycleScroll"/>//是否循环播放
        <attr format="dimension" name="lux_bannerRadius"/>//圆角
        <attr format="boolean" name="lux_hideIndicator"/>//是否隐藏indicator
        <attr format="boolean" name="lux_showIndicatorForSinglePage"/>//只有一条数据是否隐藏indicator
        <attr format="reference" name="lux_defaultImage"/>//占位图
        <attr format="boolean" name="lux_fade"/>//渐显
        <attr format="enum" name="lux_scaleType">
            <enum name="center_crop" value="0"/>
            <enum name="center" value="1"/>
            <enum name="center_inside" value="2"/>
            <enum name="fit_center" value="3"/>
            <enum name="fit_end" value="4"/>
            <enum name="fit_start" value="5"/>
            <enum name="fit_xy" value="6"/>
            <enum name="matrix" value="7"/>
        </attr>

    </declare-styleable>
    <declare-styleable name="LuxBaseCell">
        <attr format="boolean" name="cell_showSwitch"/>//是否展示开关
        <attr format="boolean" name="cell_switchEnable"/>//是否可用
        <attr format="boolean" name="cell_switchChecked"/>//开关状态

        <attr format="boolean" name="cell_showArrow"/>//是否展示箭头
        <attr format="color|reference" name="cell_arrowColor"/>//箭头颜色
        <attr format="string" name="cell_arrowText"/>//箭头
        <attr format="dimension" name="cell_arrowSize"/>//箭头大小
        <attr format="boolean" name="cell_arrowBold"/>//箭头是否加粗

        <attr format="string" name="cell_titleText"/>//标题
        <attr format="color|reference" name="cell_titleColor"/>//标题颜色
        <attr format="dimension" name="cell_titleSize"/>//标题大小
        <attr format="boolean" name="cell_titleBold"/>//标题是否加粗

        <attr format="string" name="cell_subTitleText"/>//副标题
        <attr format="color|reference" name="cell_subTitleColor"/>//副标题颜色
        <attr format="dimension" name="cell_subTitleSize"/>//副标题大小
        <attr format="boolean" name="cell_subTitleBold"/>//副标题是否加粗

        <attr format="boolean" name="cell_topLine"/>//上分割线
        <attr format="dimension" name="cell_topLineMarginLeft"/>//上分割线距左
        <attr format="dimension" name="cell_topLineMarginRight"/>//上分割线距右
        <attr format="boolean" name="cell_bottomLine"/>//下分割线
        <attr format="dimension" name="cell_bottomLineMarginLeft"/>//下分割线距左
        <attr format="dimension" name="cell_bottomLineMarginRight"/>//下分割线距右

        <attr format="dimension" name="cell_leftPadding"/>//内容左间距
        <attr format="dimension" name="cell_rightPadding"/>//内容右间距
    </declare-styleable>
    <declare-styleable name="LuxButton">
        <attr format="enum" name="lux_styleType">
            <enum name="main" value="0"/>//主按钮（默认）
            <enum name="sub" value="1"/>//次按钮
            <enum name="common" value="2"/>//通栏按钮
        </attr>

        <attr name="lux_gradientOrientation"/>// 渐变色方向

        <attr name="lux_strokeWidth"/>边框大小
        <attr name="lux_strokeColor"/>边框颜色 selector
        <attr format="color" name="lux_strokeColorSelected"/>边框颜色 选中

        <attr format="string|color" name="lux_backgroundColors_normal"/>//正常态
        <attr format="string|color" name="lux_backgroundColors_pressed"/>//按压态
        <attr format="string|color" name="lux_backgroundColors_disable"/>//置灰态
        <attr format="string|color" name="lux_backgroundColors_selected"/>//选中态

        <attr name="lux_textColor"/>文字颜色 selector
